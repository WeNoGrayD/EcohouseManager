<Window x:Class="EcohouseManager.SupportingCommand"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EcohouseManager"
        mc:Ignorable="d"
        Title="Поддерживаемые команды" Height="220" Width="330">
    <ListBox FontSize="13">
        <ListBoxItem>Модуль числа: abs(x)</ListBoxItem>
        <ListBoxItem>Квадратный корень: sqrt(x)</ListBoxItem>
        <ListBoxItem>Округление до меньшего: floor(x)</ListBoxItem>
        <ListBoxItem>Округление до большего: ceiling(x)</ListBoxItem>
        <ListBoxItem>Синус: sin(x)</ListBoxItem>
        <ListBoxItem>Косинус: cos(x)</ListBoxItem>
        <ListBoxItem>Тангенс: tan(x)</ListBoxItem>
        <ListBoxItem>Минимум: min(x;y) (не меньше 2 параметров)</ListBoxItem>
        <ListBoxItem>Максимум: max(x;y) (не меньше 2 параметров)</ListBoxItem>
    </ListBox>
</Window>
